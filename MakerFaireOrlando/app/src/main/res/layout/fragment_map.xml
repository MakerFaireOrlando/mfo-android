<?xml version="1.0" encoding="utf-8"?>

<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/map_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    <WebView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:id="@+id/webView"/>

    <LinearLayout
        android:id="@+id/map_floorcontrol"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical|right"
        android:layout_marginRight="15dp"
        android:background="@drawable/map_floor_layout_background"
        android:orientation="vertical"
        android:paddingBottom="5dp"
        android:paddingTop="5dp" >

        <Button
            android:id="@+id/map_floor4"
            style="@style/MapFloorSelectorButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/map_floorcontrol_4" />
        <Button
            android:id="@+id/map_floor3"
            style="@style/MapFloorSelectorButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/map_floorcontrol_3" />

        <Button
            android:id="@+id/map_floor2"
            style="@style/MapFloorSelectorButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/map_floorcontrol_2" />

        <Button
            android:id="@+id/map_floor1"
            style="@style/MapFloorSelectorButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/map_floorcontrol_1" />

    </LinearLayout>




</FrameLayout>